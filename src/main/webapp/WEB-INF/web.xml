<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
    <servlet>
  	<servlet-name>SearchServlet</servlet-name>
  	<display-name>SearchServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.SearchServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>RegisterCustomerServlet</servlet-name>
  	<display-name>RegisterCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.RegisterCustomerServlet</servlet-class>
  </servlet>
  
    <servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/search.do</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>RegisterCustomerServlet</servlet-name>
  	<url-pattern>/register.do</url-pattern>
  </servlet-mapping>
  
</web-app>
